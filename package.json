{
  "name": "taller-js-boilerplate",
  "version": "0.0.1",
  "description": "Taller JS Boilerplate.",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "rm -Rf dist/ && webpack",
    "watch": "rm -Rf dist/ && webpack --watch",
    "test": "jest --verbose --no-cache",
    "test-watch": "jest --watch --verbose --no-cache"
  },
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-boot": "^1.0.0",
    "redux-promise": "^0.5.3",
    "superagent": "^2.0.0",
    "superagent-promise-plugin": "^3.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "axios": "^0.11.0",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flux-standard-action": "^0.6.1",
    "gitbook-cli": "^2.1.3",
    "jest": "^12.1.1",
    "jest-cli": "^12.1.1",
    "jest-webpack-alias": "^2.3.0",
    "json-loader": "^0.5.4",
    "postcss-initial": "^1.5.1",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.1.0",
    "style-loader": "^0.13.1",
    "symbol-observable": "^0.2.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys",
      "object.values",
      "es-abstract",
      "babel-runtime"
    ],
    "moduleNameMapper": {
      "^app/(.*)": "<rootDir>/src/$1"
    }
  }
}
